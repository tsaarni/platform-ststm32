{
  "build": {
    "cpu": "cortex-m0",
    "extra_flags": "-DSTM32F030x4",
    "f_cpu": "48000000L",
    "mcu": "stm32f030f4p6",
    "ldscript": "stm32f030f4.ld"
  },
  "debug": {
    "openocd_target": "stm32f0x",
